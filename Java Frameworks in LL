import java.util.LinkedList;
public class JavaFrameworks {
    //create
    LinkedList<Integer> ll = new LinkedList<>();

    //add
    ll.addLast(1);
    ll.addFirst(2);
    ll.addFirst(0);
    //0->1->2
    System.out.println(ll);
    //remove
    ll.removeLast();
    ll.removeFirst(ll);
    System.out.println(ll);
}